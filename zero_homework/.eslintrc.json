{
  "parserOptions": {
    "ecmaVersion": 6
 },
  "env": {
    "node": true
  },
  "rules": {
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "comma-dangle": [2, "never"],
    "complexity": [0, 6],
    "curly": 0,
    "eqeqeq": 0,
    "max-statements": [2, 30],
    "no-shadow-restricted-names": 2,
    "no-undef": 2,
    "no-use-before-define": 2,
    "radix": 2,
    "semi": ["error", "never"],
    "space-infix-ops": 2,
    "strict": 0
  },
  "globals": {
    "AnalysisView": true,
    "PollingView": true,
    "Prism": true,
    "Spinner": true,
    "Timer": true,
    "moment": true
  }
}
